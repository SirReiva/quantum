{
  "presets": [
    ["@babel/typescript", { "jsxPragma": "h" }]
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-class-properties",
    ["transform-react-jsx", {
      "pragma": "h"
    }],
    ["minify-dead-code-elimination", { "optimizeRawSize": true }]
  ]
}
